terraform {
  required_providers {
    google = {
      source  = "hashicorp/google"
      version = ">=5.26.0"
    }
  }
  required_version = ">= 1.1.0"
}

provider "google" {
  project = var.gcp_project_id
  region  = var.gcp_region
}

module "gcp" {
  source  = "./gcp"
  count   = var.cfg.target == "gcp" ? 1 : 0
  options = local.options
}
