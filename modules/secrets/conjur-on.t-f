terraform {
  required_providers {
    conjur = {
      source  = "localhost/cyberark/conjur"
      version = ">=0.6.7"
    }
  }
  required_version = ">= 1.1.0"
}

module "conjur" {
  source = "./conjur"
  count  = var.cfg.secret.manager == "conjur" ? 1 : 0
  cfg    = var.cfg
}
